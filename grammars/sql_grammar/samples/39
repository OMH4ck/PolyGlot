CREATE TABLE v0 ( v1 INTEGER PRIMARY KEY ) ; CREATE TABLE v2 AS SELECT * FROM v0 AS x1 , v0 AS x2 WHERE v1 = 10 AND v1 = 3 ORDER BY v1 DESC NULLS FIRST ; CREATE TABLE v3 ( v4 INTEGER PRIMARY KEY , v5 INT ) ; INSERT INTO v3 ( v5 ) VALUES ( 10 ) ,( 0 ) ; SELECT DISTINCT * FROM v0 , v2 , v2 , v3 AS myname ; 
