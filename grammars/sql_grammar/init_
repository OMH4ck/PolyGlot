insert into child2 values ('BBB', 42);
insert into child3 values (42, 'CCC');
update parent set b = b + 1;
delete from parent;
insert into parent values ('AAA', 42);
insert into parent values ('BBB', 42);
insert into parent values ('CCC', 42);
delete from child1;
delete from child2;
delete from child3;
