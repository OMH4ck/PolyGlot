CREATE FUNCTION op_leak(int, int) RETURNS bool    AS 'BEGIN RAISE NOTICE ''op_leak => %, %'', $1, $2; RETURN $1 < $2; END'    LANGUAGE plpgsql;
DROP FUNCTION op_leak(int, int);
