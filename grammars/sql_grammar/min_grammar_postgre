---
create_view_stmt:
CREATE opt_temp VIEW view_name opt_column_name_list_p AS select_stmt opt_check_option **view_name->identifier=kDataTableName:10:Define;opt_column_name_list_p->column_name_list->column_name->identifier=kDataColumnName:11:Define**
CREATE opt_temp RECURSIVE VIEW view_name opt_column_name_list_p AS select_stmt opt_check_option **view_name->identifier=kDataTableName:10:Define;opt_column_name_list_p->column_name_list->column_name->identifier=kDataColumnName:11:Define**
CREATE OR REPLACE opt_temp VIEW view_name opt_column_name_list_p AS select_stmt opt_check_option **view_name->identifier=kDataTableName:10:Define;opt_column_name_list_p->column_name_list->column_name->identifier=kDataColumnName:11:Define**
CREATE OR REPLACE opt_temp RECURSIVE VIEW view_name opt_column_name_list_p AS select_stmt opt_check_option **view_name->identifier=kDataTableName:10:Define;opt_column_name_list_p->column_name_list->column_name->identifier=kDataColumnName:11:Define**
