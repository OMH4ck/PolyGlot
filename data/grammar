program:
stmtlist
---
stmtlist:
stmt
stmtlist stmt
---
stmt:
select_statement SEMI
create_statement SEMI
show_statement SEMI
function_statement SEMI
pragma_statement SEMI
---
pragma_statement:
PRAGMA pragma_key EQUAL pragma_value
---
pragma_key:
identifier **identifier=kDataPragmaKey:1:Global**
---
pragma_value:
identifier **identifier=kDataPragmaValue:2:Global**
---
create_statement:
CREATE tablename LD column_list RD **column_list->column_name->identifier=kDataColumnName:2:Define;tablename->identifier=kDataTableName:1:Define**
CREATE VIEW view_name **view_name->identifier=kDataViewName:1:Define**
---
function_statement:
function_name LD RD **function_name->identifier=kDataFunctionName:1:Global**
---
show_statement:
SHOW VIEW view_name **view_name->identifier=kDataViewName:1:Use**
---
select a,b from x,y
select_statement:
SELECT column_list_opt_quote FROM tablename_list **column_list_opt_quote->column_list->column_name->identifier=kDataColumnName:2:Use;tablename_list->tablename->identifier=kDataTableName:1:Use:MapToAll**
SELECT column_list_opt_quote FROM tablename_list WHERE tablename DOT column_name EQUAL tablename DOT column_name **column_list_opt_quote->column_list->column_name->identifier=kDataColumnName:2:Use;tablename_list->tablename->identifier=kDataTableName:1:Use:MapToAll;tablename_1->identifier=kDataTableName:2:Use:MapToAll;column_name_1->identifier=kDataColumnName:3:Use:MapToClosestOne;tablename_2->identifier=kDataTableName:2:Use:MapToAll;column_name_2->identifier=kDataColumnName:3:Use:MapToClosestOne**
---
function_name:
identifier
---
tablename_list:
tablename
tablename COMMA tablename_list
---
table_def:
tablename DOT column_name
---
column_list_opt_quote:
    column_list
    LD column_list RD
---
column_list:
    column_name
    column_list COMMA column_name %prec COMMA 
---
tablename:
    identifier
---
column_name:
    identifier
---
view_name:
identifier
---
identifier:
    IDENTIFIER
---
opt_value:
    int_literal

---
int_literal:
    INTLITERAL
---
op:
    OP
